#ededintersi,polindromyoxla
#include <iostream>
using namespace std;

void reverseNum(int n){
    int temp = n, rev = 0;

    while(temp > 0){
        rev = rev * 10 + temp % 10;
        temp /= 10;
    }

    cout << rev << endl;

    if(rev == n) cout << "Palindrom" << endl;
    else cout << "Palindrom deyil" << endl;
}

int main(){
    int x;
    cin >> x;
    reverseNum(x);
}
